
clock {
	 timezone EST+5EDT;
     starttime '2009-07-21 00:00:00';
     stoptime '2009-07-22 00:00:00';
}
#include "light_schedule.glm";
module powerflow {
     solver_method FBS;
     NR_iteration_limit 50;
};
module market;
module tape;
module residential {
     implicit_enduses NONE;
};
module climate;
module connection;

class auction {
    double current_price_mean_24h;
    double current_price_stdev_24h;
}

object auction {
    name Market_1;
    special_mode NONE;
    unit kW;
    price_cap 3.78;
    period 300;
    init_price 0.10;
    init_stdev 0.05;
		capacity_reference_object transformer23;
		capacity_reference_property power_out_real;
	object player {
		property capacity_reference_bid_price;
		file ExamplePrices2.player;
		loop 150;
	};
    max_capacity_reference_bid_quantity 150;
    warmup 0;
object recorder {
         property capacity_reference_bid_price, current_market.clearing_price, current_market.clearing_quantity;
         limit 100000000;
         interval 60;
         file baseprice_clearedprice_clearedquantity.csv;
    };
}




object csv_reader {
     name CSVREADER;
     filename "ColumbusWeather2009_2a.csv";
}

object climate {
     name "Minneapolis MN";
     tmyfile "ColumbusWeather2009_2a.csv";
     reader CSVREADER;
};

object fncs_msg {
     name fncs_msg;
 	 	 parent Market_1;
     route "function:controller/submit_bid_state -> auction/submit_bid_state";
     option "transport:hostname localhost, port 5570";
     configure fncs_msg.txt;
}



